{
  "name": "@tinijs/content",
  "version": "0.0.1",
  "description": "A basic file-based content system.",
  "author": "Lam Nhan",
  "homepage": "https://tinijs.dev",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": "./public-api.js",
  "types": "./public-api.d.ts",
  "files": [
    "11ty",
    "assets",
    "lib",
    "cli",
    "module",
    "tini.module.*",
    "public-api.*",
    "!**/*.ts",
    "**/*.d.ts",
    "!test"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "gts lint",
    "fix": "gts fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinijs/servers.git"
  },
  "bugs": {
    "url": "https://github.com/tinijs/servers/issues"
  },
  "peerDependencies": {
    "@tinijs/cli": "file:../cli/tinijs-cli-0.16.0.tgz"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-plugin-bundle": "^1.0.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@iarna/toml": "^2.2.5",
    "markdown-it-anchor": "^8.6.7",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.9.4",
    "gts": "^5.2.0",
    "typescript": "~5.3.2"
  }
}
